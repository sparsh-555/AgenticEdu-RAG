# Environment Configuration Template for Agentic Edu-RAG System
# Copy this file to .env and fill in your actual values

# OpenAI Configuration
OPENAI_API_KEY=your_openai_api_key_here
OPENAI_MODEL=gpt-4o
OPENAI_EMBEDDING_MODEL=text-embedding-3-small

# LangGraph Configuration
LANGGRAPH_DEBUG=false

# Vector Database Configuration
CHROMA_PERSIST_DIRECTORY=./data/chroma_db
CHROMA_COLLECTION_NAME=edu_rag_collection

# System Configuration
LOG_LEVEL=INFO
MAX_TOKENS=8000
TEMPERATURE=0.1

# Performance Configuration
MAX_CONCURRENT_REQUESTS=5
REQUEST_TIMEOUT=30
CLASSIFICATION_CONFIDENCE_THRESHOLD=0.7

# Evaluation Configuration
EVALUATION_DATASET_PATH=./data/cs1qa/processed
RESULTS_OUTPUT_DIR=./evaluation/results
